<package>
  <name>rbx2_arm_nav</name>
  <version>0.1.0</version>

  <description>ROS By Example Vol 2 - Arm Navigation</description>

  <maintainer email="patrick@pirobot.org">Patrick Goebel</maintainer>
  <license>BSD</license>
  <url type="website">http://ros.org/wiki/rbx2_arm_nav</url>
